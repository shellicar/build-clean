  "devDependencies": {
    "@shellicar/build-clean": "workspace:^",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^24.9.1",
    "npm-run-all2": "^8.0.4",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  }
}
